@c randp libinterp/corefcn/rand.cc
@anchor{XREFrandp}
@deftypefn  {Built-in Function} {} randp (@var{l}, @var{n})
@deftypefnx {Built-in Function} {} randp (@var{l}, @var{m}, @var{n}, @dots{})
@deftypefnx {Built-in Function} {} randp (@var{l}, [@var{m} @var{n} @dots{}])
@deftypefnx {Built-in Function} {@var{v} =} randp ("state")
